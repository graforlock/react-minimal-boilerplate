{
  "name": "react-minimal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "watchify src/main.js -v -t [ babelify --presets [react] ] -o public/js/main.js",
    "test": "mocha './test/**/*.spec.js' --compilers js:babel-core/register",
    "flow": "babel src --out-dir build_flow --blacklist flow && flow build_flow"
  },
  "author": "Maciej Sitko",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-plugin-flow-comments": "^6.3.19",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.2",
    "expect": "^1.14.0",
    "expect-jsx": "^2.3.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "flow-comments"
    ]
  },
  "dependencies": {
    "babel-register": "^6.3.13",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.4.5",
    "react-router": "^1.0.3",
    "redux": "^3.4.0",
    "redux-form": "^5.0.1",
    "redux-promise": "^0.5.3",
    "whatwg-fetch": "^0.10.1"
  }
}
